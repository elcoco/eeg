#!/usr/bin/python3
import plotly
#import plotly.tools as tls
#print(plotly.__version__)  # version >1.9.4 required
from plotly.graph_objs import Scatter, Layout
import numpy as np

plotly.offline.plot({
"data": [
    Scatter(x=[np.random.randn(100)], y=[4, 1, 3, 7])
],
"layout": Layout(
    title="hello world"
)
})
